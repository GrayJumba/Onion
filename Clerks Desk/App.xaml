<Application x:Class="Clerks_Desk.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"   
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/SDLibrary;component/Style/SDStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/Brown.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="MainBrush" Color="#663200"/>
            <SolidColorBrush x:Key="LightBrush" Color="Transparent"/>
            <ImageBrush x:Key="MainBackGround" ImageSource="Images/numbers.png"/>
            <sys:String xmlns:sys="clr-namespace:System;assembly=mscorlib" x:Key="AppTitle">Fees Desk</sys:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
