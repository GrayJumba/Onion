<Application x:Class="AcademicsDesk.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            
              xmlns:metro="http://metro.mahapps.com/winfx/xaml/controls"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/SDLibrary;component/Style/SDStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/Teal.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="MainBrush"  Color="Teal"/>
            <sys:String  xmlns:sys="clr-namespace:System;assembly=mscorlib" x:Key="AppTitle">Academics Desk</sys:String>
            <ImageBrush x:Key="MainBackGround" ImageSource="Images/numbers.png"/>
            <ImageBrush x:Key="Logo" ImageSource="Image/A.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
