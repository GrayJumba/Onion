<Application x:Class="TeachersDesk.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:metro="http://metro.mahapps.com/winfx/xaml/controls"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/SDLibrary;component/Style/SDStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/Blue.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="MainBrush" Color="#1976D2"/>
            <ImageBrush x:Key="MainBackGround" ImageSource="BackGround/numbers.png"/>
            <sys:String xmlns:sys="clr-namespace:System;assembly=mscorlib" x:Key="AppTitle">Teachers Desk</sys:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
